{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "access" : {
      "$ref" : "access.json"
    },
    "attributeValues" : {
      "type" : "array",
      "items" : {
        "$ref" : "attributeValue.json"
      }
    },
    "code" : {
      "type" : "string"
    },
    "created" : {
      "type" : "string",
      "format" : "date-time"
    },
    "createdBy" : {
      "$ref" : "user.json"
    },
    "description" : {
      "type" : "string"
    },
    "disabled" : {
      "type" : "boolean"
    },
    "displayName" : {
      "type" : "string"
    },
    "externalAccess" : {
      "type" : "boolean"
    },
    "favorite" : {
      "type" : "boolean"
    },
    "favorites" : {
      "type" : "array",
      "items" : {
        "type" : "string"
      }
    },
    "href" : {
      "type" : "string"
    },
    "id" : {
      "type" : "string"
    },
    "lastUpdated" : {
      "type" : "string",
      "format" : "date-time"
    },
    "lastUpdatedBy" : {
      "$ref" : "user.json"
    },
    "name" : {
      "type" : "string"
    },
    "publicAccess" : {
      "type" : "string"
    },
    "sharing" : {
      "$ref" : "sharing.json"
    },
    "source" : {
      "$ref" : "source_.json"
    },
    "targets" : {
      "type" : "array",
      "items" : {
        "anyOf" : [ {
          "$ref" : "webhookTarget.json",
          "type" : "object",
          "properties" : {
            "type" : {
              "const" : "webhook"
            }
          },
          "required" : [ "type" ]
        }, {
          "$ref" : "consoleTarget.json",
          "type" : "object",
          "properties" : {
            "type" : {
              "const" : "console"
            }
          },
          "required" : [ "type" ]
        }, {
          "$ref" : "jmsTarget.json",
          "type" : "object",
          "properties" : {
            "type" : {
              "const" : "jms"
            }
          },
          "required" : [ "type" ]
        }, {
          "$ref" : "kafkaTarget.json",
          "type" : "object",
          "properties" : {
            "type" : {
              "const" : "kafka"
            }
          },
          "required" : [ "type" ]
        } ]
      }
    },
    "translations" : {
      "type" : "array",
      "items" : {
        "$ref" : "translation.json"
      }
    },
    "user" : {
      "$ref" : "user.json"
    },
    "userAccesses" : {
      "type" : "array",
      "items" : {
        "$ref" : "userAccess.json"
      }
    },
    "userGroupAccesses" : {
      "type" : "array",
      "items" : {
        "$ref" : "userGroupAccess.json"
      }
    }
  },
  "required" : [ "disabled", "source", "targets" ]
}