{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "type" : "object",
  "properties" : {
    "complete" : {
      "type" : "boolean"
    },
    "completedTime" : {
      "type" : "string",
      "format" : "date-time"
    },
    "description" : {
      "type" : "string"
    },
    "duration" : {
      "type" : "integer"
    },
    "error" : {
      "type" : "string"
    },
    "items" : {
      "type" : "array",
      "items" : {
        "$ref" : "item.json"
      }
    },
    "onFailure" : {
      "$ref" : "failurePolicy.json"
    },
    "startedTime" : {
      "type" : "string",
      "format" : "date-time"
    },
    "status" : {
      "$ref" : "status____.json"
    },
    "summary" : {
      "type" : "string"
    },
    "totalItems" : {
      "type" : "integer"
    }
  }
}