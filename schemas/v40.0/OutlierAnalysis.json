{
  "type": "object",
  "required": [
    "normalizationMethod",
    "enabled",
    "outlierMethod"
  ],
  "properties": {
    "enabled": {
      "type": "boolean"
    },
    "extremeLines": {
      "$ref": "OutlierLine.json"
    },
    "normalizationMethod": {
      "type": "string",
      "enum": [
        "Y_RESIDUALS_LINEAR"
      ]
    },
    "outlierMethod": {
      "type": "string",
      "enum": [
        "IQR",
        "STANDARD_Z_SCORE",
        "MODIFIED_Z_SCORE"
      ]
    },
    "thresholdFactor": {
      "type": "number",
      "format": "double"
    }
  }
}
