{
  "$schema" : "https://json-schema.org/draft/2020-12/schema",
  "allOf" : [ {
    "type" : "object",
    "properties" : {
      "auth" : {
        "anyOf" : [ {
          "$ref" : "httpBasicAuth.json",
          "type" : "object",
          "properties" : {
            "type" : {
              "const" : "http-basic"
            }
          },
          "required" : [ "type" ]
        }, {
          "$ref" : "apiTokenAuth.json",
          "type" : "object",
          "properties" : {
            "type" : {
              "const" : "api-token"
            }
          },
          "required" : [ "type" ]
        } ]
      },
      "clientId" : {
        "type" : "string"
      },
      "contentType" : {
        "type" : "string"
      },
      "headers" : {
        "type" : "object",
        "$comment" : "Undefined",
        "additionalProperties" : {
          "type" : "string"
        }
      },
      "type" : {
        "type" : "string"
      },
      "url" : {
        "type" : "string"
      }
    },
    "required" : [ "clientId", "contentType", "url" ]
  }, {
    "type" : "object",
    "properties" : {
      "type" : {
        "const" : "webhook"
      }
    },
    "required" : [ "type" ]
  } ]
}