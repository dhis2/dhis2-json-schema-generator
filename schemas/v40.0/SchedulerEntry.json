{
  "type": "object",
  "required": [
    "configurable",
    "enabled",
    "status"
  ],
  "properties": {
    "configurable": {
      "type": "boolean"
    },
    "cronExpression": {
      "type": "string"
    },
    "delay": {
      "type": "integer",
      "format": "int32"
    },
    "enabled": {
      "type": "boolean"
    },
    "name": {
      "type": "string"
    },
    "nextExecutionTime": {
      "type": "string",
      "format": "date-time"
    },
    "sequence": {
      "type": "array",
      "items": {
        "$ref": "SchedulerEntryJob.json"
      }
    },
    "status": {
      "type": "string",
      "enum": [
        "RUNNING",
        "COMPLETED",
        "STOPPED",
        "SCHEDULED",
        "DISABLED",
        "FAILED",
        "NOT_STARTED"
      ]
    },
    "type": {
      "type": "string"
    }
  }
}
