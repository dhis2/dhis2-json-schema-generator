{
  "type": "object",
  "required": [
    "messageCount",
    "messageType",
    "priority",
    "status"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "assignee": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "code": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "displayName": {
      "type": "string"
    },
    "extMessageId": {
      "type": "string"
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "followUp": {
      "type": "boolean"
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "lastMessage": {
      "type": "string",
      "format": "date-time"
    },
    "lastSender": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "lastSenderFirstname": {
      "type": "string"
    },
    "lastSenderSurname": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "messageCount": {
      "type": "integer",
      "format": "int32"
    },
    "messageType": {
      "type": "string",
      "enum": [
        "PRIVATE",
        "SYSTEM",
        "VALIDATION_RESULT",
        "TICKET",
        "SYSTEM_VERSION_UPDATE"
      ]
    },
    "messages": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uid",
            "pattern": "^[0-9a-zA-Z]{11}$",
            "minLength": 11,
            "maxLength": 11,
            "example": "mX3Bw20Pq44"
          }
        },
        "description": "A UID reference to a Message  \n(Java name `org.hisp.dhis.message.Message`)"
      }
    },
    "priority": {
      "type": "string",
      "enum": [
        "NONE",
        "LOW",
        "MEDIUM",
        "HIGH"
      ]
    },
    "publicAccess": {
      "type": "string"
    },
    "read": {
      "type": "boolean"
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "status": {
      "type": "string",
      "enum": [
        "NONE",
        "OPEN",
        "PENDING",
        "INVALID",
        "SOLVED"
      ]
    },
    "subject": {
      "type": "string"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "user": {
      "type": "object",
