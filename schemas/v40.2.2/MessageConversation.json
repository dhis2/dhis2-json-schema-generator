    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userFirstname": {
      "type": "string"
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    },
    "userMessages": {
      "type": "array",
      "items": {
        "$ref": "UserMessage.json"
      }
    },
    "userSurname": {
      "type": "string"
    }
  }
}
