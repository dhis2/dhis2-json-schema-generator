        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uid",
            "pattern": "^[0-9a-zA-Z]{11}$",
            "minLength": 11,
            "maxLength": 11,
            "example": "xj2sS1ni0Q9"
          }
        },
