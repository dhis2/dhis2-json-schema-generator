{
  "type": "object",
  "required": [
    "analyticsPeriodBoundaryType"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "analyticsPeriodBoundaryType": {
      "type": "string",
      "enum": [
        "BEFORE_START_OF_REPORTING_PERIOD",
        "BEFORE_END_OF_REPORTING_PERIOD",
        "AFTER_START_OF_REPORTING_PERIOD",
        "AFTER_END_OF_REPORTING_PERIOD"
      ]
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "boundaryTarget": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "displayName": {
      "type": "string"
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "name": {
      "type": "string"
    },
    "offsetPeriodType": {
      "type": "string",
      "enum": [
        "BiMonthly",
        "BiWeekly",
        "Daily",
        "FinancialApril",
        "FinancialJuly",
        "FinancialNov",
        "FinancialOct",
        "Monthly",
        "Quarterly",
        "QuarterlyNov",
        "SixMonthlyApril",
        "SixMonthlyNov",
        "SixMonthly",
        "TwoYearly",
        "Weekly",
        "WeeklySaturday",
        "WeeklySunday",
        "WeeklyThursday",
        "WeeklyWednesday",
        "Yearly"
      ]
    },
    "offsetPeriods": {
      "type": "integer",
      "format": "int32"
    },
    "publicAccess": {
      "type": "string"
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    }
  }
}
