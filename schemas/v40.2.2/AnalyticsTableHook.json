{
  "type": "object",
  "required": [
    "phase",
    "analyticsTableType",
    "resourceTableType"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "analyticsTableType": {
      "type": "string",
      "enum": [
        "DATA_VALUE",
        "COMPLETENESS",
        "COMPLETENESS_TARGET",
        "ORG_UNIT_TARGET",
        "EVENT",
        "ENROLLMENT",
        "OWNERSHIP",
        "VALIDATION_RESULT"
      ]
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "code": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "displayName": {
      "type": "string"
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "name": {
      "type": "string"
    },
    "phase": {
      "type": "string",
      "enum": [
        "RESOURCE_TABLE_POPULATED",
        "ANALYTICS_TABLE_POPULATED"
      ]
    },
    "publicAccess": {
      "type": "string"
    },
    "resourceTableType": {
      "type": "string",
      "enum": [
        "ORG_UNIT_STRUCTURE",
        "DATA_SET_ORG_UNIT_CATEGORY",
        "CATEGORY_OPTION_COMBO_NAME",
        "DATA_ELEMENT_GROUP_SET_STRUCTURE",
        "INDICATOR_GROUP_SET_STRUCTURE",
        "ORG_UNIT_GROUP_SET_STRUCTURE",
        "CATEGORY_STRUCTURE",
        "DATA_ELEMENT_STRUCTURE",
        "PERIOD_STRUCTURE",
        "DATE_PERIOD_STRUCTURE",
        "DATA_ELEMENT_CATEGORY_OPTION_COMBO",
        "DATA_APPROVAL_REMAP_LEVEL",
        "DATA_APPROVAL_MIN_LEVEL"
      ]
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "sql": {
      "type": "string"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    }
  }
}
