{
  "type": "object",
  "required": [
    "style",
    "strategy"
  ],
  "properties": {
    "set": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "nZ5pL4Kf3gS"
        }
      },
      "description": "A UID reference to a LegendSet  \n(Java name `org.hisp.dhis.legend.LegendSet`)"
    },
    "showKey": {
      "type": "boolean"
    },
    "strategy": {
      "type": "string",
      "enum": [
        "FIXED",
        "BY_DATA_ITEM"
      ]
    },
    "style": {
      "type": "string",
      "enum": [
        "FILL",
        "TEXT"
      ]
    }
  }
}
