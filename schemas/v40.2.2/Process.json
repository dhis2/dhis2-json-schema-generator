{
  "type": "object",
  "required": [
    "duration",
    "status"
  ],
  "properties": {
    "cancelledTime": {
      "type": "string",
      "format": "date-time"
    },
    "complete": {
      "type": "boolean"
    },
    "completedTime": {
      "type": "string",
      "format": "date-time"
    },
    "description": {
      "type": "string"
    },
    "duration": {
      "type": "integer",
      "format": "int64"
    },
    "error": {
      "type": "string"
    },
    "jobId": {
      "type": "string"
    },
    "stages": {
      "type": "array",
      "items": {
        "$ref": "Stage.json"
      }
    },
    "status": {
      "type": "string",
      "enum": [
        "RUNNING",
        "SUCCESS",
        "ERROR",
        "CANCELLED"
      ]
    },
    "summary": {
      "type": "string"
    }
  }
}
