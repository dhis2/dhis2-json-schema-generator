{
  "type": "object",
  "required": [
    "imageFormat",
    "mapService",
    "mapLayerPosition"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "attribution": {
      "type": "string"
    },
    "code": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "displayName": {
      "type": "string"
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "imageFormat": {
      "type": "string",
      "enum": [
        "PNG",
        "JPG"
      ]
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "layers": {
      "type": "string"
    },
    "legendSet": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "nZ5pL4Kf3gS"
        }
      },
      "description": "A UID reference to a LegendSet  \n(Java name `org.hisp.dhis.legend.LegendSet`)"
    },
    "legendSetUrl": {
      "type": "string"
    },
    "mapLayerPosition": {
      "type": "string",
      "enum": [
        "BASEMAP",
        "OVERLAY"
      ]
    },
    "mapService": {
      "type": "string",
      "enum": [
        "WMS",
        "TMS",
        "XYZ",
        "VECTOR_STYLE",
        "GEOJSON_URL",
        "ARCGIS_FEATURE"
      ]
    },
    "name": {
      "type": "string"
    },
    "publicAccess": {
      "type": "string"
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "url": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    }
  }
}
