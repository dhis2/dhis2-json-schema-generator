{
  "type": "object",
  "required": [
    "valueType",
    "version"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "code": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "description": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "name": {
      "type": "string"
    },
    "options": {
      "type": "array",
      "items": {
        "type": "object",
        "required": [
          "id"
        ],
        "properties": {
          "id": {
            "type": "string",
            "format": "uid",
            "pattern": "^[0-9a-zA-Z]{11}$",
            "minLength": 11,
            "maxLength": 11,
            "example": "n7oq8VD7EG6"
          }
        },
        "description": "A UID reference to a Option  \n(Java name `org.hisp.dhis.option.Option`)"
      }
    },
    "publicAccess": {
      "type": "string"
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    },
