{
  "type": "object",
  "required": [
    "cacheStrategy",
    "type"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "cacheStrategy": {
      "type": "string",
      "enum": [
        "NO_CACHE",
        "CACHE_1_MINUTE",
        "CACHE_5_MINUTES",
        "CACHE_10_MINUTES",
        "CACHE_15_MINUTES",
        "CACHE_30_MINUTES",
        "CACHE_1_HOUR",
        "CACHE_6AM_TOMORROW",
        "CACHE_TWO_WEEKS",
        "RESPECT_SYSTEM_SETTING"
      ]
    },
    "code": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "description": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "name": {
      "type": "string"
    },
    "publicAccess": {
      "type": "string"
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "sqlQuery": {
      "type": "string"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "type": {
      "type": "string",
      "enum": [
        "VIEW",
        "MATERIALIZED_VIEW",
        "QUERY"
      ]
    },
    "updateJobId": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    }
  }
}
