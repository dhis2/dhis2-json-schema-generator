{
  "type": "object",
  "required": [
    "userGroup"
  ],
  "properties": {
    "access": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "userGroup": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "wd5C82cv3p6"
        }
      },
      "description": "A UID reference to a UserGroup  \n(Java name `org.hisp.dhis.user.UserGroup`)"
    },
    "userGroupUid": {
      "type": "string"
    }
  }
}
