      "$ref": "Sharing.json"
    },
    "subjectTemplate": {
      "type": "string"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
