{
  "type": "object",
  "required": [
    "user"
  ],
  "properties": {
    "access": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userUid": {
      "type": "string"
    }
  }
}
