  "required": [
    "valueType"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "code": {
      "type": "string"
    },
