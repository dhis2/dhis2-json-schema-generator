      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
