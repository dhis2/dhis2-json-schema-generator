{
  "type": "object",
  "properties": {
    "lastYears": {
      "type": "integer",
      "format": "int32"
    },
    "skipPrograms": {
      "type": "array",
      "items": {
        "type": "string",
        "format": "uid",
        "pattern": "^[a-zA-Z][a-zA-Z0-9]{10}$"
      }
    },
    "skipResourceTables": {
      "type": "boolean"
    },
    "skipTableTypes": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "DATA_VALUE",
          "COMPLETENESS",
          "COMPLETENESS_TARGET",
          "ORG_UNIT_TARGET",
          "EVENT",
          "ENROLLMENT",
          "OWNERSHIP",
          "VALIDATION_RESULT"
        ]
      }
    }
  }
}
