{
  "type": "object",
  "required": [
    "notificationTrigger",
    "notificationRecipient"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "code": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "deliveryChannels": {
      "type": "array",
      "items": {
        "type": "string",
        "enum": [
          "SMS",
          "EMAIL",
          "HTTP"
        ]
      }
    },
    "displayMessageTemplate": {
      "type": "string"
    },
    "displayName": {
      "type": "string"
    },
    "displaySubjectTemplate": {
      "type": "string"
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "messageTemplate": {
      "type": "string"
    },
    "name": {
      "type": "string"
    },
    "notificationRecipient": {
      "type": "string",
      "enum": [
        "TRACKED_ENTITY_INSTANCE",
        "ORGANISATION_UNIT_CONTACT",
        "USERS_AT_ORGANISATION_UNIT",
        "USER_GROUP",
        "PROGRAM_ATTRIBUTE",
        "DATA_ELEMENT",
        "WEB_HOOK"
      ]
    },
    "notificationTrigger": {
      "type": "string",
      "enum": [
        "ENROLLMENT",
        "COMPLETION",
        "PROGRAM_RULE",
        "SCHEDULED_DAYS_DUE_DATE",
        "SCHEDULED_DAYS_INCIDENT_DATE",
        "SCHEDULED_DAYS_ENROLLMENT_DATE"
      ]
    },
    "notifyParentOrganisationUnitOnly": {
      "type": "boolean"
    },
    "notifyUsersInHierarchyOnly": {
      "type": "boolean"
    },
    "publicAccess": {
      "type": "string"
    },
    "recipientDataElement": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "hX7n8Y4pu89"
        }
      },
      "description": "A UID reference to a DataElement  \n(Java name `org.hisp.dhis.dataelement.DataElement`)"
    },
    "recipientProgramAttribute": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "Ip8AV7eW679"
        }
      },
      "description": "A UID reference to a TrackedEntityAttribute  \n(Java name `org.hisp.dhis.trackedentity.TrackedEntityAttribute`)"
    },
    "recipientUserGroup": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "wd5C82cv3p6"
        }
      },
      "description": "A UID reference to a UserGroup  \n(Java name `org.hisp.dhis.user.UserGroup`)"
    },
    "relativeScheduledDays": {
      "type": "integer",
      "format": "int32"
    },
    "sendRepeatable": {
      "type": "boolean"
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "subjectTemplate": {
      "type": "string"
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    }
  }
}
