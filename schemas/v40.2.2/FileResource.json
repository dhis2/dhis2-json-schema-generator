{
  "type": "object",
  "required": [
    "domain",
    "contentLength",
    "storageStatus"
  ],
  "properties": {
    "access": {
      "$ref": "Access.json"
    },
    "attributeValues": {
      "type": "array",
      "items": {
        "$ref": "AttributeValue.json"
      }
    },
    "code": {
      "type": "string"
    },
    "contentLength": {
      "type": "integer",
      "format": "int64"
    },
    "contentMd5": {
      "type": "string"
    },
    "contentType": {
      "type": "string"
    },
    "created": {
      "type": "string",
      "format": "date-time"
    },
    "createdBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "displayName": {
      "type": "string"
    },
    "domain": {
      "type": "string",
      "enum": [
        "DATA_VALUE",
        "PUSH_ANALYSIS",
        "DOCUMENT",
        "MESSAGE_ATTACHMENT",
        "USER_AVATAR",
        "ORG_UNIT"
      ]
    },
    "externalAccess": {
      "type": "boolean"
    },
    "favorite": {
      "type": "boolean"
    },
    "favorites": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "hasMultipleStorageFiles": {
      "type": "boolean"
    },
    "href": {
      "type": "string"
    },
    "id": {
      "type": "string"
    },
    "lastUpdated": {
      "type": "string",
      "format": "date-time"
    },
    "lastUpdatedBy": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "name": {
      "type": "string"
    },
    "publicAccess": {
      "type": "string"
    },
    "sharing": {
      "$ref": "Sharing.json"
    },
    "storageStatus": {
      "type": "string",
      "enum": [
        "NONE",
        "PENDING",
        "FAILED",
        "STORED"
      ]
    },
    "translations": {
      "type": "array",
      "items": {
        "$ref": "Translation.json"
      }
    },
    "user": {
      "type": "object",
      "required": [
        "id"
      ],
      "properties": {
        "id": {
          "type": "string",
          "format": "uid",
          "pattern": "^[0-9a-zA-Z]{11}$",
          "minLength": 11,
          "maxLength": 11,
          "example": "r87xh9Xn8ON"
        }
      },
      "description": "A UID reference to a User  \n(Java name `org.hisp.dhis.user.User`)"
    },
    "userAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserAccess.json"
      }
    },
    "userGroupAccesses": {
      "type": "array",
      "items": {
        "$ref": "LegacyUserGroupAccess.json"
      }
    }
  }
}
